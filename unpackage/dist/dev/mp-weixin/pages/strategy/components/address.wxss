
.address-text {
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
}
.address-text image {
	width: 33rpx;
	height: 33rpx;
	padding-left: 30rpx;
}
.address {
	position: relative;
	height: 400rpx;
}
.address-list {
	position: absolute;
	left: 50rpx;
	top: 100rpx;
	z-index: 999;
}
.address-img {
	width: 100%;
	height: 400rpx;
}
.address-img image {
	width: 750rpx !important;
	height: 400rpx;
}
.words {
	font-size: 50rpx;
	color: #ffffff;
	font-weight: bold;
}
.words-eng {
	font-size: 30rpx;
	color: #ffffff;
}
.back {
	background: rgba(0, 51, 0, 0.3);
	position: absolute;
	top: 0;
	left: 0;
	height: 400rpx;
	width: 100%;
}
/* 机票 */
.Plane image {
	width: 100%;
	border-radius: 20rpx;
	height: 150rpx;
}
.Plane {
	margin: 40rpx 20rpx;
	height: 150rpx;
}

